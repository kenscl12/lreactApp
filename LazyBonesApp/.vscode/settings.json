// Place your settings in this file to overwrite the default settings
{
	// Editor settings
	"editor.tabSize": 4,
	"editor.insertSpaces": false,
	"editor.renderWhitespace": "boundary",
	//ESLint settings
	"eslint.enable": true,
	"javascript.validate.enable": true,

	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.DS_Store": true,
		"**/node_modules": true,
		"**/bower_components": true,
		"**/build": true,
		"**/babel_cache": true
	}
}