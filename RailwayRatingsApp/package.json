{
  "name": "ufs-spa",
  "version": "1.0.0",
  "description": "The best app for rail passengers",
  "main": "src/server.jsx",
  "scripts": {
    "eslint": "./node_modules/.bin/eslint",
    "lint": "npm run eslint -- ./src gulpfile.js webpack.client.config.js --quiet --ext .jsx",
    "gulp": "./node_modules/.bin/gulp",
    "webpack-dev-server": "set WATCH=true&&webpack-dev-server --progress --inline --hot --config webpack.client.config --colors",
    "wds": "npm run webpack-dev-server",
    "webpack": "./node_modules/.bin/webpack",
    "build-local": "npm run gulp -- --stand=local --minify=false --publish=false",
    "build-cms-local": "npm run gulp -- --stand=cms-local --minify=false --publish=false",
    "build-develop": "npm run gulp -- --stand=develop --minify=true --maps=true --publish=true",
    "build-test": "npm run gulp -- --stand=test --minify=true --maps=false --publish=true",
    "build-hotfix": "npm run gulp -- --stand=hotfix --minify=true --maps=false --publish=true",
    "build-production": "npm run gulp -- --stand=production --minify=true --maps=false --publish=true",
    "build-vniigt": "npm run gulp -- --stand=vniigt --minify=true --maps=false --publish=true",
    "deploy-develop": "npm run build-develop && .\\deploy\\deploy.sh static-dev.ufs-online.ru .\\build",
    "deploy-test": "npm run build-test && .\\deploy\\deploy.sh static-test.ufs-online.ru .\\build",
    "deploy-hotfix": "npm run build-hotfix && .\\deploy\\deploy.sh static-hotfix.ufs-online.ru .\\build",
    "deploy-teamcity-develop": "npm run build-develop && sh ./deploy/deploy.sh static-dev.ufs-online.ru ./build",
    "deploy-teamcity-test": "npm run build-test && sh ./deploy/deploy.sh static-test.ufs-online.ru ./build",
    "deploy-teamcity-hotfix": "npm run build-hotfix && sh ./deploy/deploy.sh static-hotfix.ufs-online.ru ./build",
    "deploy-teamcity-production": "npm run build-production && sh ./deploy/deploy.sh static0.ufs-online.ru ./build"
  },
  "author": "Universal Financial Systems, LLC",
  "license": "ISC",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^7.2.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^0.28.7",
    "del": "^3.0.0",
    "eslint": "^4.12.1",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "gulp": "^3.9.1",
    "gulp-cli": "^1.4.0",
    "handlebars": "^4.0.11",
    "json-loader": "^0.5.7",
    "modernizr-loader": "^1.0.1",
    "node-sass": "^4.7.2",
    "postcss": "^6.0.14",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.9",
    "resolve-url-loader": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.9.1",
    "webpack-dev-server": "^2.9.5",
    "webpack-modernizr-loader": "^4.0.0",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "classnames": "^2.2.5",
    "es6-promise": "^4.1.1",
    "jsonloader": "^1.0.1",
    "mkdirp": "^0.5.1",
    "modernizr": "^3.5.0",
    "moment": "^2.19.3",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-cookie": "^2.1.2",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
